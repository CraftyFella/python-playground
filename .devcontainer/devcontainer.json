{
    "name": "Python Workshop Environment",
    "dockerComposeFile": "docker-compose.yml",
    "service": "workshop-env",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "forwardPorts": [
        8888
    ],
    "hostRequirements": {
        "cpus": 2
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "customizations": {
        "codespaces": {
            "openFiles": [
                "README.md",
                "hello_world.py"
            ]
        },
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ]
        }
    }
}